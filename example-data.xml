<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
  <dns-server
      xmlns="http://www.nic.cz/ns/yang/dns-server"
      xmlns:tsig="http://www.nic.cz/ns/yang/tsig-algorithms"
      xmlns:knot="http://www.nic.cz/ns/yang/knot-dns">
    <description>Example DNS server configuration.</description>
    <server-options>
      <chaos-identity/>
      <nsid-identity/>
      <version/>
      <listen-endpoint>
	<name>01</name>
	<ip-address>192.0.0.1</ip-address>
	<port>6378</port>
      </listen-endpoint>
      <listen-endpoint>
	<name>02</name>
	<ip-address>::</ip-address>
      </listen-endpoint>
    </server-options>
    <access-control-list>
      <name>simple</name>
      <access-list-entry>
        <name>01</name>
        <prefix>192.168.2.100/32</prefix>
      </access-list-entry>
    </access-control-list>
    <access-control-list>
      <name>mixed</name>
      <access-list-entry>
        <name>01</name>
        <prefix>192.168.2.0/32</prefix>
      </access-list-entry>
      <access-list-entry>
        <name>02</name>
        <prefix>2001:db8::1/128</prefix>
      </access-list-entry>
    </access-control-list>
    <access-control-list>
      <name>key</name>
      <access-list-entry>
        <name>01</name>
	<key>key1</key>
      </access-list-entry>
    </access-control-list>
    <key>
      <name>key1</name>
      <algorithm>tsig:hmac-sha512</algorithm>
      <secret>Wg==</secret>
    </key>
  </dns-server>
</data>
